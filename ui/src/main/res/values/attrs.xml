<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="drawableSize" format="dimension" />
    <declare-styleable name="EditTextExtend">
        <attr name="drawableSize"/>
        <attr name="hasTextShow" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="ResizeDrawableCheckBox">
        <attr name="drawableSize"/>
        <attr name="checkDrawable" format="reference"/>
    </declare-styleable>
    <declare-styleable name="ToolbarExtend">
        <attr name="drawableSize"/>
        <attr name="titleGravity">
            <flag name="center_horizontal" value="1" />
            <flag name="left" value="3" />
            <flag name="right" value="5" />
            <flag name="center_vertical" value="16" />
            <flag name="center" value="17" />
        </attr>
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
</resources>